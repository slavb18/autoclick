{
  "name": "@ilb/autoclick",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "clean": "rimraf --no-glob ./.next ./out",
    "check:es:build": "es-check es5 './.next/static/**/*.js' -v",
    "escheck": "npm run clean && npm run build && npm run check:es:build",
    "lint": "eslint ./pages ./next.config.js -f table --ext .js --ext .jsx || true",
    "test": "jest",
    "_publish": "npm install && npm run build && npm run check:es:build && npm unpublish @ilb/autoclick@1.0.0 && npm publish"
  },
  "dependencies": {
    "@ilb/jsonschemaform": "^1.0.3",
    "next": "^9.5.5",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^4.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "semantic-ui-offline": "^2.4.1-1",
    "semantic-ui-react": "^0.84.0",
    "uniforms": "^3.0.0-rc.8",
    "uniforms-bridge-json-schema": "^3.0.0-rc.8",
    "uniforms-semantic": "^3.0.0-rc.8"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "es-check": "^5.1.1",
    "eslint": "^7.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.21.4",
    "file-loader": "^3.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0"
  },
  "license": "MIT"
}
